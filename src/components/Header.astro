---
import ButtonHeader from '../components/ButtonHeader.astro'
---

<div class="relative bg-white">
  <div class="mx-auto max-w-7xl px-6">
    <div
      class="flex items-center justify-between border-b-2 border-gray-100 py-6 md:justify-start md:space-x-10"
    >
      <div class="flex justify-start lg:w-0 lg:flex-1">
        <a href="#">
          <span class="sr-only">Your Company</span>
          <img
            class="w-auto h-8 sm:h-10"
            src="https://esmeraldareservanatural.com/images/Logo.png"
            alt=""
          />
        </a>
      </div>
      <div>
        <ButtonHeader name="Servicios" />
        <ButtonHeader name="Galería" />
        <ButtonHeader name="Noticias" />
        <ButtonHeader name="Contactanos" />
        <ButtonHeader name="Apóyanos" />
      </div>
    </div>
  </div>
</div>
