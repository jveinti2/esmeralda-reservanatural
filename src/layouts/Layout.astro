---
import Footer from "../components/Footer.astro";
import Header from "../components/Header.astro";
import Header2 from "../components/Header2.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en" class="scroll-smooth">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/icono2.ico" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <link
    href="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.3/flowbite.min.css"
    rel="stylesheet"
    />
    <script
    src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/1.6.3/flowbite.min.js"
    ></script>
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
    href="https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;500;700&display=swap"
    rel="stylesheet"
    />
    <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />
    <script src="/main.js" type="module"></script>
    <style>
      body {
        font-family: "Roboto", sans-serif;
      }
      </style></head
  >

  <body>
    <!-- <Header /> -->
    <Header2 />

    <slot />
    
    <Footer />

  </body>
</html>
